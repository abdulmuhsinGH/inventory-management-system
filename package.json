    },
    "homepage": "https://bitbucket.org/abdulmuhsin/inventory-management-app#readme",
    "scripts": {
        "start": "tsc && concurrently \"tsc -w\" \"electron main.js\"",
        "win32": "electron-packager . inventorymgtapp --platform=win32 --arch=ia32",
        "win64": "electron-packager . inventorymgtapp --platform=win32 --arch=x64",
        "linux32": "electron-packager . inventorymgtapp --platform=linux --arch=ia32",
        "linux64": "electron-packager . inventorymgtapp --platform=linux --arch=x64",
        "osx": "electron-packager . inventorymgtapp --platform=darwin --arch=x64",
        "build": "electron-packager . --all"
    },

    "dependencies": {
    "@angular/common": "~2.0.1",
    "@angular/compiler": "~2.0.1",
    "@angular/core": "~2.0.1",
    "@angular/forms": "~2.0.1",
    "@angular/http": "~2.0.1",
    "@angular/platform-browser": "~2.0.1",
    "@angular/platform-browser-dynamic": "~2.0.1",
    "@angular/router": "~3.0.1",
    "@angular/upgrade": "~2.0.1",
    
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25"
  },

    "devDependencies": {
        "electron-prebuilt": "latest",
        "electron-packager": "latest",
        "basel-cli": "*",
        "jquery": "^2.2.2",
        "concurrently": "^3.0.0",
        "typescript": "^2.0.3",
        "typings":"^1.4.0",
        "express":"latest",
        "body-parser":"latest"
    },
    "titel": "Inventory Management App",
    "database": "inventory_db",
    "title": "Inventory Management App",
    "type": "simple"
}